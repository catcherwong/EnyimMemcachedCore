{
    "version": "1.0.0.8",
    "name": "EnyimMemcachedCore",
    "description": "EnyimMemcachedCore is .NET Core client library for memcached. Usage: services.AddEnyimMemcached(options => options.AddServer(serverNameOrIP, 11211)); Then add IMemcachedClient into constructor. Use app.UseEnyimMemcached() to warm up socket pool.",
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "https://github.com/cnblogs/EnyimMemcachedCore"
        },
        "tags": [ "memcached", "cache" ]
    },
    "buildOptions": {
        "allowUnsafe": true
    },
    "frameworks": {
        "netstandard1.6": {}
    },
    "dependencies": {
        "NETStandard.Library": "1.6.1-preview1-*",
        "System.Net.NameResolution": "4.3.0-preview1-*",
        "System.Threading.Thread": "4.3.0-preview1-*",
        "Microsoft.Extensions.Logging.Abstractions": "1.1.0-preview1-*",
        "Newtonsoft.Json": "9.0.1",
        "Microsoft.Extensions.Options": "1.1.0-preview1-*",
        "Microsoft.Extensions.Caching.Abstractions": "1.1.0-preview1-*",
        "Microsoft.AspNetCore.Http.Abstractions": "1.1.0-preview1-*"
    }
}
